@model ContosoUniversity.Tender

@{
    ViewBag.Title = "Подробности";
}

<h2>Подробности</h2>

<div>
    <h4>Тендер</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Имя")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.name)
        </dd>

        <dt>
            @Html.DisplayName("Описание")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.description)
        </dd>

        <dt>
            @Html.DisplayName("Минимальная ставка")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.minPrice)
        </dd>

        <dt>
            @Html.DisplayName("Максимальная ставка")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.maxPrice)
        </dd>



    </dl>
</div>
<p>
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Назад", "Index")
</p>
